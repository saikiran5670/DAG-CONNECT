											"physicalType": "integer"
										},
										"sink": {
											"name": "target_org_id",
											"type": "Int32",
											"physicalType": "integer"
										}
									},
									{
										"source": {
											"name": "start_date",
											"type": "Double",
											"physicalType": "double precision"
										},
										"sink": {
											"name": "start_date",
											"type": "Int64",
											"physicalType": "bigint"
										}
									},
									{
										"source": {
											"name": "created_at",
											"type": "Double",
											"physicalType": "double precision"
										},
										"sink": {
											"name": "created_at",
											"type": "Int64",
											"physicalType": "bigint"
										}
									},
									{
										"source": {
											"name": "chg_flg",
											"type": "String",
											"physicalType": "text"
										},
										"sink": {
											"name": "chg_flg",
											"type": "String",
											"physicalType": "character varying"
										}
									},
									{
										"source": {
