{
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "ConnectionString": "#{DAFConnectMasterDatabaseConnectionString}#",
    "DataMartConnectionString": "#{DAFConnectDataMartDatabaseConnectionString}#"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "System": "Information",
      "Grpc": "Information",
      "Microsoft": "Information"
    }
  },
  "PurgingConfiguration": {
    "IsWebServiceCall": "#{IsWebServiceCall}#",
    "ThreadSleepTimeInSec": "#{ThreadSleepTimeInSec}#",
    "RetryCount": "#{RetryCount}#",
    "WebServiceRetryCount": "#{WebServiceRetryCount}#",
    "CancellationTokenDuration": "#{CancellationTokenDuration}#"
  }
}
