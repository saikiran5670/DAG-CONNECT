{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "ServiceConfiguration": {
    "accountservice": "http://accountservice",
    "packageservice": "http://localhost:5000",
    "featureservice": "http://localhost:5009",
    "organizationservice": "http://localhost:5010",
    //"packageservice": "http://packageservice",
    //"featureservice": "http://featureservice",
    //"organizationservice": "http://organizationservice",
    "roleservice": "http://roleservice",
    "vehicleservice": "http://vehicleservice",
    "auditservice": "http://auditservice",
    "translationservice": "http://translationservice",
    "driverservice": "http://driverservice"
  },

  "WebServerConfiguration": {
    "isdevelopmentenv": "true",
    "cookiesexpireat": "10",
    "authcookiesexpireat": "10",
    "headercachecontrol": "no-cache, no-store, must-revalidate",
    "headerexpires": "-1",
    "headerpragma": "no-cache",
    "headerxframeoptions": "DENY",
    "headerxxssprotection": "1",
    "headerstricttransportsecurity": "31536000",
    "headeraccesscontrolalloworigin": "*",
    "headeraccesscontrolallowmethods": "GET, POST, PUT, DELETE",
    "headeraccesscontrolallowheaders": "Content-Type, Access-Control-Allow-Headers, Authorization, X-Requested-With"
  },
  "AllowedHosts": "*"
}
