{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "ServiceConfiguration": {
    "accountservice": "http://accountservice",
    "packageservice": "http://packageservice",
    "featureservice": "http://featureservice",
    "organizationservice": "http://organizationservice",
    "roleservice": "http://roleservice",
    "vehicleservice": "http://vehicleservice",
    "auditservice": "http://auditservice",
    "translationservice": "http://translationservice",
    "driverservice": "http://driverservice"
  },
  "WebServerConfiguration": {
    "isdevelopmentenv": "#{ServerConfigurationIsDevelopmentEnv}#",
    "cookiesexpireat": "#{ServerConfigurationCookiesExpireAt}#",
    "authcookiesexpireat": "#{ServerConfigurationAuthCookiesExpireAt}#",
    "headercachecontrol": "#{ServerConfigurationHeaderCacheControl}#",
    "headerexpires": "#{ServerConfigurationHeaderExpires}#",
    "headerpragma": "#{ServerConfigurationHeaderPragma}#",
    "headerxframeoptions": "#{ServerConfigurationHeaderXFrameOptions}#",
    "headerxxssprotection": "#{ServerConfigurationHeaderXXssProtection}#",
    "headerstricttransportsecurity": "#{ServerConfigurationHeaderStrictTransportSecurity}#",
    "headeraccesscontrolalloworigin": "#{ServerConfigurationHeaderAccessControlAllowOrigin}#",
    "headeraccesscontrolallowmethods": "#{ServerConfigurationHeaderAccessControlAllowMethods}#",
    "headeraccesscontrolallowheaders": "#{ServerConfigurationHeaderAccessControlAllowHeaders}#",
    "httpsport": "#{ServerConfigurationHttpsPort}#"
  },
  "PortalCacheConfiguration": {
    "expiryinmilliseconds": "#{PortalCacheConfigurationExpiryInMilliSeconds}#",
    "expiryinseconds": "#{PortalCacheConfigurationExpiryInSeconds}#",
    "expiryinminutes": "#{PortalCacheConfigurationExpiryInMinutes}#",
    "expiryinhours": "#{PortalCacheConfigurationExpiryInHours}#",
    "expiryindays": "#{PortalCacheConfigurationExpiryInDays}#",
    "expiryinmonths": "#{PortalCacheConfigurationExpiryInMonths}#",
    "expiryinyears": "#{PortalCacheConfigurationExpiryInYears}#",
    "isslidingexpiration": "#{PortalCacheConfigurationIsSlidingExpiration}#",
    "isabsoluteexpiration": "#{PortalCacheConfigurationIsAbsoluteExpiration}#"
  },
  "AllowedHosts": "*"
}